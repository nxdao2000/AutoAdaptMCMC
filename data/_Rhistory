class(runList$crossLevel)
runList$crossLevel <- {
conf <- configureMCMC(oldConf = abModel$initialMCMCconf)
conf$setSamplers()
conf$addSampler(type = "crossLevel", target = c("a[1]", "b[1]"),
print = FALSE)
conf$addSampler(type = "crossLevel", target = c("a[2]", "b[2]"),
print = FALSE)
conf
}
